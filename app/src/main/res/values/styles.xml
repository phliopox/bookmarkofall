<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="diphy_text_default">
        <item name="android:fontFamily">@font/diphylleia_regular</item>
        <item name="android:textColor">@color/text_color</item>
    </style>

    <style name="bookmark_text" parent="diphy_text_default">
        <item name="android:textColor">@color/apricot_dark</item>
        <item name="android:textSize">9sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="monte" >
        <item name="android:fontFamily">@font/montserrat_medium</item>
        <item name="android:textColor">@color/text_color</item>
    </style>
    <style name="monte_9sp" parent="monte">
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/cloudy_more_dark</item>
    </style>

    <style name="DialogSlideUpAnimation">
        <item name="android:windowExitAnimation">@anim/slide_up</item>
    </style>
</resources>